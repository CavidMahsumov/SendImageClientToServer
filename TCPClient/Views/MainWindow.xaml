<Window x:Class="TCPClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TCPClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid >
        <StackPanel>
            <Button Command="{Binding SelectBtnCommand}" Name="SelectImageButton" Width="300" Content="Select" Height="60"></Button>
            <Button Name="SendImageButton" Width="300" Content="Send" Height="60"></Button>
        </StackPanel>
    </Grid>
</Window>
